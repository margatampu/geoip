{
  "name": "sabartampubolon/geoip",
  "type": "library",
  "description": "Simple Geo Location Information using IP Address",
  "keywords": [
    "geo",
    "location",
    "ip address",
    "geoplugin"
  ],
  "homepage": "https://github.com/sabartampubolon/geoip",
  "license": "MIT",
  "authors": [
    {
      "name": "sabartampubolon",
      "email": "sabartampubolon@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.6|~7.0"
  },
  "autoload": {
    "psr-4": {
      "Sabartampubolon\\Geoip\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sabartampubolon\\Geoip\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.5-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
